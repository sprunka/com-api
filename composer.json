{
    "name": "sprunka/com-api",
    "description": "This is an API for use with City of Mist",
    "minimum-stability": "dev",
    "license": "MIT",
    "version": "0.0.1",
    "authors": [
        {
            "name": "Sean Prunka",
            "email": "sprunka@gmail.com"
        },
        {
            "name": "Vampyre Bytes",
            "email": "admin@vampyrebytes.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/sprunka/CommonRoutes"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "slim/slim": "^4.5",
        "slim/psr7": "^1.2",
        "php-di/php-di": "dev-master",
        "selective/basepath": "dev-master",
        "zircote/swagger-php": "^3.1",
        "fakerphp/faker": "^1.15",
        "sprunka/common-routes": "*"
    },
    "require-dev" : {
        "phpunit/phpunit": "^9.4",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "CoMAPI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CoMAPI\\Test\\": "tests/"
        }
    }
}
